{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
	"strict": true,
	"strictNullChecks": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
	"resolveJsonModule": true,
	"isolatedModules": true,
	"outDir": "./dist",
    "skipLibCheck": true,
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
